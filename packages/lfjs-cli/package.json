{
  "name": "lfjs-cli",
  "version": "0.5.1",
  "description": "LFJS command line",
  "homepage": "https://lfjs.io",
  "author": "Paul Chavard",
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "lint": "eslint src",
    "test": "npm run lint",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build && npm test"
  },
  "bin": {
    "lfjs": "./bin/lfjs.js",
    "lfjs-node": "./bin/lfjs-node.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfjs/lfjs.git"
  },
  "keywords": [
    "es6",
    "modules"
  ],
  "bugs": {
    "url": "https://github.com/lfjs/lfjs/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "eslint": "^3.2.2"
  },
  "dependencies": {
    "commander": "^2.8.1",
    "lfjs-core": "^0.5.1",
    "lfjs-register": "^0.5.0",
    "lodash": "^4.5.1",
    "path-is-absolute": "^1.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
