{
  "name": "lfjs-core",
  "version": "0.5.1",
  "description": "LFJS compiler core",
  "homepage": "https://lfjs.io",
  "author": "Paul Chavard",
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "lint": "eslint src test/*.js",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "npm test -- --watch",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfjs/lfjs.git"
  },
  "keywords": [
    "es6",
    "modules"
  ],
  "bugs": {
    "url": "https://github.com/lfjs/lfjs/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "eslint": "^3.2.2",
    "mocha": "^3.0.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-plugin-lodash": "^3.2.6",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-types": "^6.5.2",
    "lfjs-parser": "^0.5.1",
    "lfjs-runtime": "^0.5.1",
    "lodash": "^4.15.0",
    "mz": "^2.4.0"
  }
}
