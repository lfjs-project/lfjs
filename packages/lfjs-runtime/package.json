{
  "name": "lfjs-runtime",
  "version": "0.5.1",
  "description": "LFJS runtime",
  "homepage": "https://lfjs.io",
  "author": "Paul Chavard",
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "lint": "eslint src test",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-register",
    "build": "npm run build:node && npm run build:browser",
    "build:node": "babel src --out-dir dist",
    "build:browser": "PACKAGE_NAME=lfjs-runtime NODE_ENV=production rollup -c ../../rollup.config.js",
    "build:browser:test": "PACKAGE_NAME=lfjs-runtime NODE_ENV=test rollup -c ../../rollup.config.js",
    "prepublish": "npm run clean && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfjs/lfjs.git"
  },
  "keywords": [
    "es6",
    "modules"
  ],
  "bugs": {
    "url": "https://github.com/lfjs/lfjs/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "eslint": "^3.2.2",
    "mocha": "^3.0.2",
    "rollup": "^0.34.10",
    "lfjs-transducers": "^0.5.1"
  },
  "dependencies": {
    "lodash": "^4.15.0"
  }
}
