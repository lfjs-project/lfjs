{
  "name": "lfjs-runtime",
  "version": "0.5.1",
  "description": "LFJS runtime",
  "homepage": "https://lfjs.io",
  "author": "Paul Chavard",
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "lint": "eslint src test",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfjs/lfjs.git"
  },
  "keywords": [
    "es6",
    "modules"
  ],
  "bugs": {
    "url": "https://github.com/lfjs/lfjs/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "eslint": "^3.2.2",
    "mocha": "^3.0.2"
  },
  "babel": {
    "plugins": [
      "lodash"
    ],
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "any-promise": "^1.3.0",
    "lodash": "^4.5.1"
  }
}
