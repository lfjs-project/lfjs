{
  "name": "lfjs-html",
  "version": "0.5.1",
  "description": "LFJS html",
  "homepage": "https://lfjs.io",
  "author": "Paul Chavard",
  "license": "MIT",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "browser": "dist/browser/index.js",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "lint": "eslint src test",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-register",
    "test:ci": "testem:ci",
    "test:server": "testem",
    "build": "npm run build:node && npm run build:browser",
    "build:node": "babel src --out-dir dist",
    "build:browser": "rollup -c",
    "prepublish": "npm run clean && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lfjs/lfjs.git"
  },
  "keywords": [
    "es6",
    "modules"
  ],
  "bugs": {
    "url": "https://github.com/lfjs/lfjs/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "chai": "^3.5.0",
    "eslint": "^3.2.2",
    "loader.js": "^4.0.11",
    "mocha": "^3.0.2",
    "rollup": "^0.34.10",
    "testem": "^1.10.4"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  }
}
